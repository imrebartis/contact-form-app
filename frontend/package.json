{
  "name": "contact-form-app-frontend",
  "homepage": "https://imrebartis.github.io/contact-form-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite",
    "start": "pnpm run build:dev && cross-env NODE_ENV=development vite preview --mode development",
    "build": "pnpm run format && pnpm run build:noscript && tsc && vite build",
    "build:dev": "pnpm run format && pnpm run build:noscript && tsc && cross-env NODE_ENV=development vite build --mode development",
    "build:noscript": "sass --no-source-map src/styles/noscript.scss:src/styles/noscript.css --style compressed",
    "sass": "sass --no-source-map src/styles/style.scss:src/styles/index.css",
    "sass:watch": "sass --no-source-map --watch src/styles/style.scss:src/styles/index.css",
    "build:sass": "sass --no-source-map src/styles/style.scss:src/styles/index.css --style compressed",
    "test": "vitest --config vitest.config.ts",
    "test:ui": "vitest --ui --config vitest.config.ts",
    "test:run": "vitest run --config vitest.config.ts",
    "test:watch": "vitest --config vitest.config.ts",
    "test:coverage": "vitest run --coverage --config vitest.config.ts",
    "lint": "eslint . && prettier --check \"./**/*.{js,ts}\"",
    "lint:fix": "eslint . --fix && prettier --write \"./**/*.{js,ts,tsx,json,scss,css}\"",
    "lint:sass": "stylelint src/styles/**/*.scss",
    "lint:sass:fix": "stylelint src/styles/**/*.scss --fix",
    "format": "prettier --write \"./**/*.{js,ts,tsx,json,scss,css}\"",
    "ts-check": "tsc"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.20.0",
    "@ianvs/prettier-plugin-sort-imports": "3.4.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.7",
    "@types/toastify-js": "^1.12.3",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "1.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^16.0.0",
    "happy-dom": "^17.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.5.0",
    "prettier": "2.8.8",
    "sass": "^1.86.0",
    "serve": "^14.2.4",
    "stylelint": "^16.14.1",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-scss": "^6.11.0",
    "terser": "^5.39.0",
    "typescript": "5.3.3",
    "vite": "^6.2.3",
    "vitest": "^1.2.1"
  },
  "dependencies": {
    "toastify-js": "^1.12.0",
    "yup": "^1.6.1"
  }
}
